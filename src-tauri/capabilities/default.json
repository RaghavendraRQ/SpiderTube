{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "fs:allow-appcache-read",
    "fs:allow-appcache-read-recursive",
    {
      "identifier": "fs:scope",
      "allow": [
        "$APPCACHE/**"
      ]
    }
  ]
}